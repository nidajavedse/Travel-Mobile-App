<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewBookedFlightsActivity"
    android:background="@color/light_grey"
    >

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:text="@string/bookedflights"
        android:textColor="@color/black"
        android:textSize="26sp"
        android:textStyle="bold"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="396dp"
        android:layout_height="152dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/roundbar"
        android:elevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/roundbar" />



    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/upcoming"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="@string/from"
        android:textColor="@color/grey"
        android:textSize="12sp"
        android:textStyle="bold"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:text="@string/islamabad"
        android:textColor="@color/orange"
        android:textSize="16sp"
        android:textStyle="bold"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/to"
        android:textColor="@color/grey"
        android:textSize="12sp"
        android:textStyle="bold"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:text="@string/dubai"
        android:textColor="@color/orange"
        android:textSize="16sp"
        android:textStyle="bold"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/textView1"
        app:layout_constraintTop_toBottomOf="@+id/textView1" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="396dp"
        android:layout_height="118dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/roundbar"
        android:elevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/roundbar" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="6dp"
        android:text="@string/isl"
        android:textColor="@color/orange"
        android:textSize="24dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toEndOf="@+id/imageViewturkish"
        app:layout_constraintTop_toTopOf="@+id/imageViewturkish" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:text="@string/dxb"
        android:textColor="@color/orange"
        android:textSize="24dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView15"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <ImageView
        android:id="@+id/imageViewturkish"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="17dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toTopOf="@+id/imageView15"
        app:srcCompat="@drawable/turkishairlines" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/islamabad"
        android:textColor="@color/grey"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        app:srcCompat="@drawable/route" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dubai"
        android:textColor="@color/grey"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:text="@string/timing"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintStart_toEndOf="@+id/imageView18"
        app:layout_constraintTop_toBottomOf="@+id/imageViewturkish" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="19dp"
        android:layout_height="17dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintStart_toStartOf="@+id/imageViewturkish"
        app:layout_constraintTop_toBottomOf="@+id/imageViewturkish"
        app:srcCompat="@android:drawable/ic_menu_recent_history" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/turkishairline"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/imageViewturkish" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="396dp"
        android:layout_height="118dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/roundbar"
        android:elevation="2dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView15"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toBottomOf="@+id/imageView15"
        app:srcCompat="@drawable/roundbar" />

    <ImageView
        android:id="@+id/imageViewqatar"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="17dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintStart_toStartOf="@+id/imageView19"
        app:layout_constraintTop_toTopOf="@+id/imageView19"
        app:srcCompat="@drawable/qatar" />

    <TextView
        android:id="@+id/textViewisl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/isl"
        android:textColor="@color/orange"
        android:textSize="24dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/textViewdxb"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textViewdxb" />

    <TextView
        android:id="@+id/textViewdxb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="6dp"
        android:text="@string/dxb"
        android:textColor="@color/orange"
        android:textSize="24dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toEndOf="@+id/imageViewqatar"
        app:layout_constraintTop_toTopOf="@+id/imageViewqatar" />

    <TextView
        android:id="@+id/textViewdubai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dubai"
        android:textColor="@color/grey"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/textViewdxb"
        app:layout_constraintTop_toBottomOf="@+id/textViewdxb" />

    <TextView
        android:id="@+id/textViewislamabad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/islamabad"
        android:textColor="@color/grey"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/textViewisl"
        app:layout_constraintTop_toBottomOf="@+id/textViewisl" />

    <ImageView
        android:id="@+id/imageViewairplane"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/textViewdxb"
        app:layout_constraintEnd_toStartOf="@+id/textViewisl"
        app:layout_constraintStart_toEndOf="@+id/textViewdxb"
        app:layout_constraintTop_toTopOf="@+id/textViewdxb"
        app:srcCompat="@drawable/route" />

    <ImageView
        android:id="@+id/imageViewclock"
        android:layout_width="19dp"
        android:layout_height="17dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintStart_toStartOf="@+id/imageViewqatar"
        app:layout_constraintTop_toBottomOf="@+id/imageViewqatar"
        app:srcCompat="@android:drawable/ic_menu_recent_history" />

    <TextView
        android:id="@+id/textViewtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:text="@string/time"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintStart_toEndOf="@+id/imageViewclock"
        app:layout_constraintTop_toBottomOf="@+id/imageViewqatar" />

    <TextView
        android:id="@+id/textViewqatar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/qatarairways"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintStart_toEndOf="@+id/textViewtime"
        app:layout_constraintTop_toBottomOf="@+id/imageViewqatar" />

</androidx.constraintlayout.widget.ConstraintLayout>